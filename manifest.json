{
	"name": "thecotne",
	"version": "2.1.0",
	"manifest_version": 2,
	"description": "thecotne",
	"icons": {
		"16": "icons/icon16.png",
		"19": "icons/icon19.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"content_scripts": [{
		"js": ["google/google.js"],
		"include_globs": ["*://*.google.*/*"],
		"matches": ["<all_urls>"],
		"run_at": "document_start"
	}, {
		"js": ["scroll/scroll.js"],
		"css": ["scroll/scroll.css"],
		"matches": ["<all_urls>"],
		"run_at": "document_start"
	}, {
		"js": ["youtube/youtube.js"],
		"matches": ["*://*.youtube.com/*"],
		"run_at": "document_end"
	}, {
		"css": ["myvideo/myvideo.css"],
		"js": ["myvideo/myvideo.js"],
		"matches": ["*://*.myvideo.ge/*"],
		"run_at": "document_end"
	}, {
		"js": ["facebook/facebook.js"],
		"matches": ["*://*.facebook.com/*"],
		"run_at": "document_end"
	}]
}
