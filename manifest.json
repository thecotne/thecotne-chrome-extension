{
	"name": "thecotne",
	"version": "2.0.2",
	"manifest_version": 2,
	"description": "thecotne",
	"icons": {
		"16": "icons/icon16.png",
		"19": "icons/icon19.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"include_globs": ["*://*.google.*/*"],
		"js": ["google.js"],
		"run_at": "document_start"
	}, {
		"matches": ["<all_urls>"],
		"js": ["scroll.js"],
		"css": ["scroll.css"],
		"run_at": "document_start"
	}, {
		"js": ["youtube.js"],
		"matches": ["*://*.youtube.com/*"],
		"run_at": "document_end"
	}, {
		"css": ["myvideo.css"],
		"js": ["myvideo.js"],
		"matches": ["*://*.myvideo.ge/*"],
		"run_at": "document_end"
	}, {
		"js": ["facebook.js"],
		"matches": ["*://*.facebook.com/*"],
		"run_at": "document_end"
	}]
}
