{
  "name": "thecotne",
  "version": "3.0.4",
  "manifest_version": 2,
  "description": "extension for thecotne",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [{
    "js": ["google/google.js"],
    "include_globs": ["*://*.google.*/*"],
    "matches": ["<all_urls>"],
    "run_at": "document_start"
  }, {
    "css": ["forumge/forumge.css"],
    "js": ["forumge/forumge.js"],
    "matches": ["*://forum.ge/*"],
    "run_at": "document_end"
  }, {
    "js": ["scroll/scroll.js"],
    "css": ["scroll/scroll.css"],
    "matches": ["<all_urls>"],
    "run_at": "document_end"
  }],
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}
